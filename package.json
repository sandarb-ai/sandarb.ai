{
  "name": "openint",
  "version": "0.2.0",
  "private": false,
  "description": "Open-source tool for capturing prompts and context for AI agents. Organizations, A2A agent registry, prompts & contexts, dark/light theme. All UI exposed as API.",
  "author": "OpenInt",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/openint-ai/openint"
  },
  "keywords": [
    "ai",
    "agents",
    "context",
    "llm",
    "prompt-engineering",
    "prompt-management",
    "context-injection",
    "mcp",
    "model-context-protocol",
    "a2a",
    "agent-to-agent",
    "ai-ops",
    "llmops"
  ],
  "scripts": {
    "sandarb": "node scripts/start-sandarb.js",
    "dev": "concurrently -n ui,api -c blue,green \"npm run dev:ui\" \"npm run dev:api\"",
    "dev:ui": "cross-env NEXT_PUBLIC_API_URL=http://localhost:4001 next dev -p 4000",
    "dev:api": "next dev -p 4001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "mcp:stdio": "node dist/mcp-stdio.js",
    "db:init-pg": "node scripts/init-postgres.js",
    "db:seed-pg": "node scripts/seed-postgres.js",
    "db:reset-context-pg": "node scripts/migrate-postgres-reset-context.js",
    "db:migrate-and-seed-pg": "node scripts/migrate-postgres-reset-context.js && node scripts/init-postgres.js && node scripts/seed-postgres.js",
    "db:full-reset-pg": "node scripts/full-reset-postgres.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:security": "vitest run tests/security"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/api-logs": "^0.211.0",
    "@opentelemetry/exporter-logs-otlp-http": "^0.211.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.211.0",
    "@opentelemetry/resources": "^2.5.0",
    "@opentelemetry/sdk-logs": "^0.211.0",
    "@opentelemetry/sdk-node": "^0.211.0",
    "@opentelemetry/sdk-trace-node": "^2.5.0",
    "@opentelemetry/semantic-conventions": "^1.39.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "diff": "^8.0.3",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.400.0",
    "next": "^14.2.0",
    "next-themes": "^0.3.0",
    "pg": "^8.18.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "recharts": "^2.12.0",
    "tailwind-merge": "^2.3.0",
    "uuid": "^9.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/diff": "^7.0.2",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.16.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "@vitest/coverage-v8": "^2.1.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^9.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
